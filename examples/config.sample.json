{
  "backend": {
    "url": "https://lassostage.textalk.se/backend/jsonrpc/v1"
  },
  "environment": {
    "host": {
      "type": "regexp",
      "pattern": "(.*)\\.[a-z]+stage\\.textalk\\.se:9999$"
    }
  },
  "main_component": {
    "name": "tws-theme",
    "version": "*"
  },
  "registry": {
    "type": "Local",
    "options": {
      "base_path": "/home/lasso/components.local"
    }
  },
  "server": {
    "bind": "0.0.0.0",
    "port": 9999
  },
  "settings": {
    "source": "examples/settings.sample.json"
  }
}
