{
  "backend": {
    "url": "https://shop.textalk.se/backend/jsonrpc/v1"
  },
  "defaults": {
    "main_component": {
      "name": "tws-theme",
      "version": "*"
    },
    "settings": {
      "source": "examples/settings.sample.json"
    }
  },
  "environment": {
    "host": {
      "type": "regexp",
      "pattern": "(.+).diversity:9999$"
    }
  },
  "logging": {
    "access": "logs/access.log",
    "error": "logs/error.log"
  },
  "registry": {
    "type": "Local",
    "options": {
      "base_path": "/home/lasso/components.local"
    }
  },
  "server": {
    "bind": "0.0.0.0",
    "port": 9999
  }
}
